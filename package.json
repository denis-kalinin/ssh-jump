  "bugs": {
    "url": "https://github.com/denis-kalinin/ssh-jump/issues"
  },
  "homepage": "https://github.com/denis-kalinin/ssh-jump#readme",
  "devDependencies": {
    "@types/asn1": "^0.2.0",
    "@types/debug": "^4.1.7",
    "@types/inquirer": "^8.1.3",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.1.0",
